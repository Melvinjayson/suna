# Atlas AI Frontend Environment Configuration for Render.com
# Copy these variables to your Render.com service environment variables

# Environment Configuration
NODE_ENV=production
NEXT_PUBLIC_ENV_MODE=production
NEXT_PUBLIC_VERCEL_ENV=production

# Backend API Configuration
NEXT_PUBLIC_BACKEND_URL=https://atlas-ai-backend.onrender.com/api
NEXT_PUBLIC_API_URL=https://atlas-ai-backend.onrender.com/api

# Frontend URL
NEXT_PUBLIC_URL=https://atlas-ai-frontend.onrender.com

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Optional: Google OAuth
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id

# OpenAI for frontend features (if needed)
OPENAI_API_KEY=your_openai_api_key

# Port Configuration
PORT=3000
