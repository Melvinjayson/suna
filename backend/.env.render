# Atlas AI Backend Environment Configuration for Render.com
# Copy these variables to your Render.com service environment variables

# Environment Mode
ENV_MODE=production

# Database Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Redis Configuration (use Render Redis or external service)
REDIS_HOST=your_redis_host
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
REDIS_SSL=true

# RabbitMQ (if using external service)
RABBITMQ_HOST=your_rabbitmq_host
RABBITMQ_PORT=5672

# LLM API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key
OPENAI_API_KEY=your_openai_api_key
MODEL_TO_USE=anthropic/claude-3-7-sonnet-latest

# Atlas AI Assistant APIs
OPENWEATHER_API_KEY=your_openweather_api_key
TAVILY_API_KEY=your_tavily_api_key
SERP_API_KEY=your_serp_api_key

# Optional LLM Providers
GROQ_API_KEY=your_groq_api_key
OPENROUTER_API_KEY=your_openrouter_api_key

# AWS Configuration (if needed)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION_NAME=us-east-1

# Data APIs
RAPID_API_KEY=your_rapid_api_key

# Web Scraping
FIRECRAWL_API_KEY=your_firecrawl_api_key
FIRECRAWL_URL=https://api.firecrawl.dev

# Container Provider
DAYTONA_API_KEY=your_daytona_api_key
DAYTONA_SERVER_URL=https://app.daytona.io/api
DAYTONA_TARGET=us

# Monitoring & Analytics
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
LANGFUSE_SECRET_KEY=your_langfuse_secret_key
LANGFUSE_HOST=https://cloud.langfuse.com

# Email Service
MAILTRAP_API_TOKEN=your_mailtrap_api_token
MAILTRAP_SENDER_EMAIL=noreply@atlas-ai.dev
MAILTRAP_SENDER_NAME=Atlas AI Team

# Storage Configuration
REMINDER_STORAGE_FILE=/app/data/reminders.json

# Frontend URL (set after frontend deployment)
NEXT_PUBLIC_URL=https://atlas-ai-frontend.onrender.com

# Performance Configuration
WORKERS=4
THREADS=2
WORKER_CONNECTIONS=1000

# Security
PYTHONPATH=/app
